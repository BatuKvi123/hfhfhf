-- StarterGui.ChatIt!.ChatItMain.1.LocalScript
local function C_4()
local script = G2L["4"];
	script.Parent.MouseButton1Down:Connect(function()
		game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(script.Parent.Parent.TextBox.Text ,"All")
	end)
end;
task.spawn(C_4);
-- StarterGui.ChatIt!.ChatItMain.Loop.LocalScript
local function C_9()
local script = G2L["9"];
	script.Parent.MouseButton1Down:Connect(function()
		while true do
			
			wait(script.Parent.Parent.Time.Text)
			game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(script.Parent.Parent.TextBox.Text ,"All")
		end
	end)
end;
task.spawn(C_9);
-- StarterGui.ChatIt!.ChatItMain.LocalScript
local function C_a()
local script = G2L["a"];
	script.Parent.Active = true
	script.Parent.Selectable = true
	script.Parent.Draggable = true
end;
task.spawn(C_a);
-- StarterGui.ChatIt!.ChatItMain.3.LocalScript
local function C_f()
local script = G2L["f"];
	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Loop.LocalScript.Enabled = false
	end)
	
end;
task.spawn(C_f);
-- StarterGui.ChatIt!.ChatItMain.4.LocalScript
