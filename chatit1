-- StarterGui.ChatIt!.ChatItMain.OPENSENCLOSE.LocalScript
local function C_23()
local script = G2L["23"];
	script.Parent.MouseButton1Click:Connect(function()
	
		if script.Parent.Parent.Sentences.Visible == true then
			script.Parent.Parent.Sentences.Visible = false
			script.Parent.Text = "Open Sentences"
		else
			script.Parent.Parent.Sentences.Visible = true
			script.Parent.Text = "Close Sentences"
		end
	end)
end;
task.spawn(C_23);
-- StarterGui.ChatIt!.Note.lfn.LocalScript
local function C_2b()
local script = G2L["2b"];
	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Parent.Note:Destroy()
		script.Parent.Parent.Parent.ChatItMain.Visible = true
	end)
end;
task.spawn(C_2b);
-- StarterGui.ChatIt!.Note.agree.LocalScript
local function C_2e()
local script = G2L["2e"];
	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.lfn.Visible = false
		script.Parent.Parent.notagree.Visible = true
	end)
end;
task.spawn(C_2e);
-- StarterGui.ChatIt!.Note.notagree.LocalScript
local function C_30()
local script = G2L["30"];
	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.lfn.Visible = true
		script.Parent.Parent.agree.Visible = true
	end)
end;
task.spawn(C_30);

